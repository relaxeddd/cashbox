                android:id="@+id/container_login_terminal_login"
                android:layout_width="0dp"
                android:layout_height="@dimen/size_52"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginEnd="@dimen/size_16"
                android:background="@{safeUnbox(viewModel.isSelectedFieldLogin) ? @drawable/edit_text_border_selected : @drawable/edit_text_border}"
                app:layout_constraintEnd_toStartOf="@+id/container_login_terminal_password"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent">


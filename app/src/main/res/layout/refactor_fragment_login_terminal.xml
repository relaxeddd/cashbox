                    android:id="@+id/text_login_terminal_login"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/size_8"
                    android:hint="@string/login"
                    android:text="@{viewModel.textLogin}"
                    android:onClick="@{(view) -> viewModel.selectField(EditFields.LOGIN)}"
                    android:focusable="false"
                    android:singleLine="true"
                    app:met_hideUnderline="true"
                    app:met_iconLeft="@drawable/username_icon"
                    app:met_iconPadding="0dp" />

            </FrameLayout>

            <FrameLayout
                android:id="@+id/container_login_terminal_password"
                android:layout_width="0dp"
                android:layout_height="@dimen/size_52"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginStart="@dimen/size_16"

<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:fancy="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".refactor.ActivityMain">

    <data>

        <import type="android.view.View"/>
        <import type="ru.cashbox.android.refactor.login_terminal.ViewModelLoginTerminal.EditFields"/>
        <variable
            name="viewModel"
            type="ru.cashbox.android.refactor.login_terminal.ViewModelLoginTerminal" />
        <variable
            name="clickListenerLogin"
            type="android.view.View.OnClickListener"/>
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="ContentDescription">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/container_login_terminal_toolbar"
            android:layout_width="0dp"
            android:layout_height="@dimen/size_52"
            android:background="@color/colorPrimary"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/image_login_terminal_logo"
                android:layout_width="@dimen/size_30"
                android:layout_height="@dimen/size_30"
                android:layout_marginStart="@dimen/size_16"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/logo" />

            <ImageView
                android:layout_width="@dimen/size_1"
                android:layout_height="@dimen/size_30"
                android:layout_marginStart="@dimen/size_16"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/image_login_terminal_logo"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@android:color/darker_gray" />

            <!--<ImageButton
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginEnd="@dimen/size_8"
                android:adjustViewBounds="true"
                android:background="?selectableItemBackgroundBorderless"
                android:padding="@dimen/size_5"
                android:scaleType="fitXY"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/settings" />-->

        </androidx.constraintlayout.widget.ConstraintLayout>

        <FrameLayout
            android:id="@+id/container_login_terminal_domain"
            android:layout_width="match_parent"
            android:layout_height="@dimen/size_52"
            android:layout_marginStart="@dimen/size_150"
            android:layout_marginTop="@dimen/size_16"
            android:layout_marginEnd="@dimen/size_150"
            android:background="@{safeUnbox(viewModel.isSelectedFieldDomain) ? @drawable/edit_text_border_selected : @drawable/edit_text_border}"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/container_login_terminal_toolbar">

            <com.rengwuxian.materialedittext.MaterialEditText
                android:id="@+id/text_login_terminal_domain"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/size_8"
                android:hint="@string/cashbox_domain"
                android:onClick="@{(view) -> viewModel.selectField(EditFields.DOMAIN)}"
                android:text="@{viewModel.textDomain}"
                android:singleLine="true"
                android:focusable="false"
                app:met_hideUnderline="true"
                app:met_iconLeft="@drawable/username_icon"
                app:met_iconPadding="0dp" />

        </FrameLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/container_login_terminal_login_password"
            android:layout_width="match_parent"
            android:layout_height="@dimen/size_52"
            android:layout_marginStart="@dimen/size_150"
            android:layout_marginTop="@dimen/size_16"
            android:layout_marginEnd="@dimen/size_150"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/container_login_terminal_domain">

